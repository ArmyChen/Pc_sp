                if (i == 'pre') {
                    targetIndex = currentIndex - 1;
                } else if (i == 'next') {
                    targetIndex = currentIndex + 1;
                } else {
                    i = i - 1;
                    targetIndex = i;
                }

                if (targetIndex >= 4 || targetIndex < 0) {
                    if (!vipType) {
                        $('.pay-trigger').trigger("click");
